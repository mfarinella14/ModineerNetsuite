						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "pages",
					"value": {
						"value": "@range(0,add(div(sub(int(activity('Get Pages').output.totalResults),mod(int(activity('Get Pages').output.totalResults),1000)),1000),1))",
						"type": "Expression"
					}
				}
			}
