{
	"name": "InforTest1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT * FROM sys.Columns",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Infor_Visual",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SinkCSV",
						"type": "DatasetReference",
						"parameters": {
							"FileSystem": "unified-data-cluster",
							"Directory": "Infor/TestData",
							"FileName": "ColumnList.csv"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Pulverman Testing"
		},
		"annotations": []
	}
}